{
  "name": "vue3-form-json-schema",
  "version": "3.0.0-alpha.7",
  "description": "Vue3 package for using forms with JSON schema",
  "author": "Tobias Järvelöv <tobias@jarvelov.se>",
  "contibutors": [
    "miguelzapaton",
    "others"
  ],
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "builddev": "vite build --sourcemap",
    "lint": "eslint src"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "vite": "^3.2.5",
    "vue": "^3.2.45"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-form-json-schema.umd.js",
  "module": "./dist/vue3-form-json-schema.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-form-json-schema.es.js",
      "require": "./dist/vue3-form-json-schema.umd.js"
    }
  },
  "peerDependencies": {
    "vue": "3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-plugin-jsx-a11y",
        "eslint-plugin-react-hooks",
        "eslint-plugin-react",
        "rollup"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelzapaton/vue3-form-json-schema.git"
  },
  "bugs": {
    "url": "https://github.com/miguelzapaton/vue3-form-json-schema/issues"
  },
  "homepage": "https://github.com/miguelzapaton/vue3-form-json-schema"
}
